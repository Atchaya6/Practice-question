QUESTION 7.

import java.util.Scanner;
public class pothu{
    public static void main(String args[])
    {
        // hungry + icecream = eat;
        boolean Hungry = "True";
        boolean Icecream ="True";
        if(Hungry && Icecream)
        {
            System.out.print("Eat");
        }
        else{
            System.out.println("not Eat");
        }
    }
}
QUESTION 8.

import java.util.Scanner;
public class atchaya{
 public static void main(String args[])
    {
        int score = 68;
        if(score<50);
        {
            System.out.print("Need to improve");
        }
        elseif(score>=50 && score<=70)
        {
         System.out.print("Good job");
        }
        elseif{
            System.out.print("Excellent");
        }
    }
}
QUESTION 9.

import java.util.Scanner;
class main{
    public static void main(String args[])
    {
        // cricket or football = play
        boolean cricket = true;
        boolean football = true;
        if(cricket && football)
        {
            System.out.print("Play");
        }
        else{
            System.out.print("Dont paly");
        }
    }
}
