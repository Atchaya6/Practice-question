// Source code is decompiled from a .class file using FernFlower decompiler (from Intellij IDEA).
class main {
   main() {
   }

   public static void main(String[] var0) {
      byte var1 = 12;
      long var2 = 234567L;
      System.err.println(var1);
      System.out.print(var2);
   }
}










